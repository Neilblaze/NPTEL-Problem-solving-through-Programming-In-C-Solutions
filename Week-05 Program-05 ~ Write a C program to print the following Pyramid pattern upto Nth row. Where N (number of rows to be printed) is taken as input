//  Output pattern like:   
*****
****
***
**
*//

#include<stdio.h>
int main()
{
int N;
scanf("%d", &N); /*The value of N is taken as input from the test case */
    int i, j;

    for(i=N; i>=1; --i)
    {
        for(j=1; j<=i; ++j)
        {
            printf("*");
        }
        printf("\n");
    }
    
    return 0;
}
